{
  "name": "eye-time",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "release": "vue-cli-service build && electron-builder",
    "burild:multi": "vue-cli-service build && electron-builder -mwl",
    "lint": "vue-cli-service lint",
    "dev": "NODE_ENV=DEV vue-cli-service serve & sleep 5 && NODE_ENV=DEV electron index.js"
  },
  "build": {
    "productName": "eye-time",
    "appId": "com.eye-time.app",
    "directories": {
      "output": "build"
    },
    "files": [
      "index.js",
      "dist/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@types/howler": "^2.0.5",
    "howler": "^2.1.1",
    "vue": "^2.5.21",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "@vue/cli-plugin-typescript": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.21"
  }
}
